{
  "name": "phpsa/laravel-yourls-plugin",
  "description": "Plugin which integrates Laravel with Yourls (Your Own URL Shortener).",
  "license": "MIT",
  "keywords": [
    "laravel",
    "yourls",
	"shorturl",
	"URL Shortener"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Craig Smith",
      "email": "vxdhost@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1|^8.0",
	"illuminate/support": "~5.8.0|^6.0|^7.0|^8.0|^9.0",
	"guzzlehttp/guzzle": "^7.0.1"
  },
  "require-dev": {
	"orchestra/testbench": "^4.0|^5.0|^6.0|^7.0",
    "phpunit/phpunit": "^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Phpsa\\LaravelYourlsPlugin\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phpsa\\LaravelYourlsPlugin\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Phpsa\\LaravelYourlsPlugin\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
